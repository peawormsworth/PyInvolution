    "from involution.algebra import Octonion\n",
    "from jupyter_test import *\n",
    "\n",
    "# from any random quaternion...\n",
    "o = random_vector(Octonion)\n",
    "\n",
    "# calculate the product of itself with its conjugate...\n",
    "calculate = o * o.conj()\n",
